      // console.log("WPP SORTEADO : ", whatsapp);

      history.pushState(
        {},
        `${name} | Dounttouch.com.br`,
        `/${slug}`
      );

      let template = "";

      for (i = 0; i < gallery.data.length; i++) {
        if (i === 0) {
          template += `
